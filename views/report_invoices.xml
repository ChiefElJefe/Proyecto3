<odoo>
    <template id="report_invoices">
        <t t-call="web.html_container">
            <t t-foreach="docs" t-as="doc">
                <h2>Arribas COMS</h2>
                <t t-esc="doc.name"/>
                <t t-esc="doc.first_lastname"/>
                <t t-esc="doc.second_lastname"/>
                <t t-esc="doc.sell_date"/>
                <t t-esc="doc.street"/>
                <t t-esc="doc.portal"/>
                <t t-esc="doc.door"/>
                <t t-esc="doc.city"/>
                <t t-esc="doc.country_id.name"/>
                <t t-esc="doc.province_id.name"/>
                <t t-esc="doc.zip"/>
                <t t-foreach="doc.keycode_ids" t-as="keys">
                    <t t-esc="keys.code_id.name"/>
                    <t t-esc="keys.key_code"/>
                    <t t-esc="keys.creation_date"/>
                    <t t-esc="keys.sell_date"/>
                </t>
                <t t-esc="doc.total_sum"/>
            </t>
        </t>
    </template>
</odoo>