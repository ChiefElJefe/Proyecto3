<odoo>
    <data>
        <record model="ir.ui.view" id="proyecto3_invoiceline_tree">
            <field name="name">proyecto3.invoiceline.tree</field>
            <field name="model">proyecto3.invoiceline</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="code_id"/>
                    <field name="key_code"/>
                    <field name="creation_date"/>
                    <field name="sell_date"/>
                    <field name="selled"/>
                </tree>
            </field>
        </record>


        <record model="ir.actions.act_window" id="proyecto3_invoiceline_action">
            <field name="name">Invoiceline</field>
            <field name="res_model">proyecto3.invoiceline</field>
            <field name="view_mode">tree</field>
        </record>
    </data>
</odoo>